# Required configuration for Startale integration

# Soneium Minato RPC endpoints
RPC_URL=https://soneium-minato.rpc.scs.startale.com?apikey=your_api_key_here
# Optional WebSocket endpoint
# RPC_WSS_URL=wss://soneium-minato.rpc.scs.startale.com?apikey=your_api_key_here

# Startale bundler configuration
BUNDLER_URL=https://your_bundler_url_here

# Startale paymaster service
PAYMASTER_SERVICE_URL=https://your_paymaster_service_url_here

# Private key for the account abstraction wallet
OWNER_PRIVATE_KEY=your_wallet_private_key_here